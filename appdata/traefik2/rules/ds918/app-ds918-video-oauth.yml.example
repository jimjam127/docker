http:
  routers:
    synology-video-rtr:
      rule: "Host(`video.{{env "DOMAINNAME_DS918"}}`)"
      entryPoints:
        - https
      middlewares:
        - chain-oauth
      service: synology-video-svc
      tls:
        certResolver: dns-cloudflare
        options: tls-opts@file
  services:
    synology-video-svc:
      loadBalancer:
        servers:
          - url: "http://192.168.1.254:5003"
